<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefWidth="100.0" styleClass="progress_panel" stylesheets="@DarkTheme.css"
      xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="-Infinity" prefHeight="60.0" vgrow="ALWAYS"/>
            </rowConstraints>
            <children>
                <Label style="-fx-font-size: 24; -fx-text-fill: #FF7A2F;" text="Command Received"/>
                <Label fx:id="commandReceived" maxWidth="1.7976931348623157E308" prefHeight="50.0"
                       styleClass="command-received" wrapText="true" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
                       GridPane.vgrow="ALWAYS">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <padding>
                        <Insets left="10.0" right="10.0"/>
                    </padding>
                </Label>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
